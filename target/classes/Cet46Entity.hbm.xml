<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="study.CET.Cet46Entity" table="cet46" schema="" catalog="weixin">
		<id name="xh">
			<column name="XH" sql-type="varchar" length="15" not-null="true"/>
		</id>
		<property name="sumScore">
			<column name="sumScore" sql-type="float" length="12"/>
		</property>
		<property name="listening">
			<column name="Listening" sql-type="varchar" length="10"/>
		</property>
		<property name="reading">
			<column name="Reading" sql-type="varchar" length="10"/>
		</property>
		<property name="compre">
			<column name="Compre" sql-type="varchar" length="10"/>
		</property>
		<property name="essay">
			<column name="Essay" sql-type="varchar" length="10"/>
		</property>
		<property name="garde">
			<column name="Garde" sql-type="varchar" length="2"/>
		</property>
		<property name="name">
			<column name="Name" sql-type="varchar" length="15"/>
		</property>
	</class>
</hibernate-mapping>