<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="play.shudong.MyShuDongEntity" table="myshudong" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" length="10" not-null="true"/>
			<generator class="increment"/>
		</id>
		<property name="seeCount">
			<column name="seeCount" sql-type="int" length="10" not-null="true"/>
		</property>
		<property name="content">
			<column name="content" sql-type="longtext" length="2147483647"/>
		</property>
		<property name="date">
			<column name="date" sql-type="varchar" length="25"/>
		</property>
		<property name="xh">
			<column name="XH" sql-type="varchar" length="20"/>
		</property>
	</class>
</hibernate-mapping>