<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="study.lecture.MyEventEntity" table="myevent" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" length="10" not-null="true"/>
		</id>
		<property name="name">
			<column name="name" sql-type="varchar"/>
		</property>
		<property name="manager">
			<column name="manager" sql-type="varchar"/>
		</property>
		<property name="runDate">
			<column name="runDate" sql-type="varchar"/>
		</property>
		<property name="runLocation">
			<column name="runLocation" sql-type="varchar"/>
		</property>
		<property name="intro">
			<column name="intro" sql-type="longtext" length="2147483647"/>
		</property>
		<property name="otherInfo">
			<column name="otherInfo" sql-type="longtext" length="2147483647"/>
		</property>
		<property name="target">
			<column name="target" sql-type="int" length="10"/>
		</property>
	</class>
</hibernate-mapping>