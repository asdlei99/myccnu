<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="play.shop.ShopItemsEntity" table="shopitems" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" not-null="true"/>
		</id>
		<property name="name">
			<column name="name" sql-type="varchar"/>
		</property>
		<property name="price">
			<column name="price" sql-type="float" precision="10"/>
		</property>
		<property name="des">
			<column name="des" sql-type="text" length="65535"/>
		</property>
		<property name="picUrl">
			<column name="picURL" sql-type="varchar"/>
		</property>
		<property name="date">
			<column name="date" sql-type="varchar"/>
		</property>
		<property name="tag">
			<column name="tag" sql-type="int"/>
		</property>
		<property name="ownerName">
			<column name="ownerName" sql-type="varchar"/>
		</property>
		<property name="ownerXh">
			<column name="ownerXH" sql-type="varchar"/>
		</property>
		<property name="ownerPhone">
			<column name="ownerPhone" sql-type="varchar"/>
		</property>
		<property name="ownerQq">
			<column name="ownerQQ" sql-type="varchar"/>
		</property>
		<property name="seeCount">
			<column name="seeCount" sql-type="int"/>
		</property>
	</class>
</hibernate-mapping>