<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="play.shudong.MyShuDongBgImgEntity" table="myshudongbgimg" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" not-null="true"/>
			<generator class="increment"/>
		</id>
		<property name="picUrl">
			<column name="picUrl" sql-type="varchar" length="100" not-null="true"/>
		</property>
		<property name="author">
			<column name="author" sql-type="varchar" length="30"/>
		</property>
		<property name="des">
			<column name="des" sql-type="text" length="65535"/>
		</property>
		<property name="xh">
			<column name="XH" sql-type="varchar" length="15"/>
		</property>
		<property name="date">
			<column name="date" sql-type="varchar" length="25"/>
		</property>
		<property name="likeCount">
			<column name="likeCount" sql-type="int"/>
		</property>
	</class>
</hibernate-mapping>