			<key-property name="jiHaoLou">
				<column name="JiHaoLou" sql-type="varchar" length="10" not-null="true"/>
			</key-property>
			<key-property name="xinQiJi">
				<column name="XinQiJi" sql-type="int" not-null="true"/>
			</key-property>
			<key-property name="diJiJieKe">
				<column name="DiJiJieKe" sql-type="int" not-null="true"/>
			</key-property>
			<key-property name="classroom">
				<column name="Classroom" sql-type="varchar" length="20" not-null="true"/>
			</key-property>
		</composite-id>
	</class>
</hibernate-mapping>