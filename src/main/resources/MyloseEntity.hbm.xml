<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="life.lose.MyLoseEntity" table="mylose" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" length="10" not-null="true"/>
		</id>
		<property name="myType">
			<column name="MyType" sql-type="tinyint" length="3"/>
		</property>
		<property name="myState">
			<column name="MyState" sql-type="tinyint" length="3"/>
		</property>
		<property name="myDes">
			<column name="MyDes" sql-type="longtext" length="2147483647"/>
		</property>
		<property name="myPhone">
			<column name="MyPhone" sql-type="varchar" length="25"/>
		</property>
		<property name="myDate">
			<column name="MyDate" sql-type="varchar" length="20"/>
		</property>
		<property name="myLocation">
			<column name="MyLocation" sql-type="varchar"/>
		</property>
		<property name="myXh">
			<column name="MyXH" sql-type="varchar" length="20"/>
		</property>
	</class>
</hibernate-mapping>